{
    "manifest_version": 3,
    "name": "insquoo",
    "version": "1.0.7",
    "description": "The simplicity of notes meets the power of rich-text documents. A Markdown-based editor built for speed, clarity, and focus.",
    "icons": {
      "48": "images/vire-logo-icon-48.png",
      "128": "images/vire-logo-icon-128.png",
      "512": "images/vire-logo-icon-512.png",
      "1024": "images/vire-logo-icon-1024.png"
    },
    "host_permissions": [
        "https://*.insquoo.com/*"
    ],
    "chrome_url_overrides": {
      "newtab": "index.html"
    }
  }